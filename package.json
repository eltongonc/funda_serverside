    "express": "^4.13.3",
    "express-handlebars": "^3.0.0",
    "morgan": "^1.6.1",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.0",
    "gulp-livereload": "^3.8.0",
    "gulp-nodemon": "^2.0.2",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^3.0.0",
    "mocha": "^3.0.2",
    "ngrok": "^2.2.6",
